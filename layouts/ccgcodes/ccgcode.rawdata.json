{{- $.Scratch.Set "json" (dict) -}}
{{- $.Scratch.Delete "ccglist" -}}
{{- $.Scratch.Add "ccglist" (slice) -}}
{{- range .Pages -}}
{{- partial "ccg/rawdata-full.json" . -}}
{{- with (.Scratch.Get "rawdata") -}}{{- $.Scratch.Add "ccglist" . -}}{{- end -}}
{{- end -}}
{{- $.Scratch.SetInMap "json" "items" ($.Scratch.Get "ccglist") -}}
{{- $.Scratch.Get "json" | jsonify -}}
